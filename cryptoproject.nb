Cell["\<\
As expected the computation does not finish in reasonable time.
We therefore implement Pollard\[CloseCurlyQuote]s p-1 method.\
\>", "Text",
 CellChangeTimes->{{3.885469585692626*^9, 3.885469601496973*^9}, {
  3.886656676521578*^9, 
  3.88665669063081*^9}},ExpressionUUID->"befd4a1d-5b53-4492-b022-\
3db468796297"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Pollard", "=", 
   RowBox[{"Function", "[", 
    RowBox[{"N", ",", " ", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"a", ",", "j", ",", "d", ",", "success", ",", "save"}], "}"}],
        ",", 
       RowBox[{
        RowBox[{"success", "=", "0"}], ";", 
        RowBox[{"a", "=", "1"}], ";", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"success", "==", "0"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"a", "=", 
            RowBox[{"a", "+", "1"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "a", "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"save", "=", "a"}], ";", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"j", "=", "2"}], ",", 
             RowBox[{"j", "<", "4000000"}], ",", 
             RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"a", "=", 
               RowBox[{"PowerMod", "[", 
                RowBox[{"a", ",", "j", ",", "N"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"d", "=", 
               RowBox[{"GCD", "[", 
                RowBox[{
                 RowBox[{"a", "-", "1"}], ",", "N"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"1", "<", "d"}], "&&", 
                 RowBox[{"d", "<", "N"}]}], ",", 
                RowBox[{
                 RowBox[{"success", "=", "1"}], ";", 
                 RowBox[{"Return", "[", "d", "]"}], ";", "Break"}]}], 
               "]"}]}]}], "\[IndentingNewLine]", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"a", " ", "=", " ", "save"}]}]}], "]"}]}]}], "]"}]}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.885470129532238*^9, 3.8854704687490788`*^9}, {
