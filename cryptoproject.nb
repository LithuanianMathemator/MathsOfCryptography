 RowBox[{
  RowBox[{"CodeToText", "=", 
   RowBox[{"Function", "[", 
    RowBox[{"x", ",", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"nmb", ",", "lst", ",", "tmp"}], "}"}], ",", 
       RowBox[{
        RowBox[{"nmb", "=", "x"}], ";", "\[IndentingNewLine]", 
        RowBox[{"lst", "=", 
         RowBox[{"{", "}"}]}], ";", 
        RowBox[{"tmp", "=", "0"}], ";", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"nmb", "!=", "0"}], ",", 
          RowBox[{
           RowBox[{"tmp", "=", 
            RowBox[{"Mod", "[", 
             RowBox[{"nmb", ",", 
              RowBox[{"2", "^", "8"}]}], "]"}]}], ";", 
           RowBox[{"lst", "=", 
            RowBox[{"Append", "[", 
             RowBox[{"lst", ",", "tmp"}], "]"}]}], ";", 
           RowBox[{"nmb", "=", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"nmb", "-", "tmp"}], ")"}], "/", 
             RowBox[{"2", "^", "8"}]}]}]}]}], "]"}], ";", 
        RowBox[{"FromCharacterCode", "[", "lst", "]"}]}]}], "]"}]}], "]"}]}], 
