Cell[BoxData[
 RowBox[{
  RowBox[{"Smoothen", "=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"g", ",", "h", ",", "p", ",", "b"}], "}"}], ",", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "Lp", ",", "st", ",", "gh", ",", "k", ",", "ght", ",", "Lghe", ",", 
         "j", ",", "ghe"}], "}"}], ",", 
       RowBox[{
        RowBox[{"Lp", "=", 
         RowBox[{"Map", "[", 
          RowBox[{"Prime", ",", 
           RowBox[{"Range", "[", "b", "]"}]}], "]"}]}], ";", 
        RowBox[{"st", "=", "0"}], ";", 
        RowBox[{"gh", "=", "h"}], ";", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"k", "=", "0"}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"k", "<", "p"}], "&"}], "st"}], "==", "0"}], ",", 
          RowBox[{
           RowBox[{"ght", "=", "gh"}], ";", 
           RowBox[{"Lghe", "=", 
            RowBox[{"{", "}"}]}], ";", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"j", "=", "0"}], ",", 
             RowBox[{"j", "<", 
              RowBox[{"Length", "[", "Lp", "]"}]}], ",", 
             RowBox[{"j", "++"}], ",", 
             RowBox[{
              RowBox[{"ghe", "=", 
               RowBox[{"IntegerExponent", "[", 
                RowBox[{"ght", ",", 
                 RowBox[{"Lp", "[", 
                  RowBox[{"[", 
                   RowBox[{"j", "+", "1"}], "]"}], "]"}]}], "]"}]}], ";", 
              RowBox[{"Lghe", "=", 
               RowBox[{"Append", "[", 
                RowBox[{"Lghe", ",", "ghe"}], "]"}]}], ";", 
              RowBox[{"ght", "=", 
               RowBox[{"ght", "/", 
                RowBox[{
                 RowBox[{"Lp", "[", 
                  RowBox[{"[", 
                   RowBox[{"j", "+", "1"}], "]"}], "]"}], "^", 
                 "ghe"}]}]}]}]}], "]"}], ";", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"ght", "==", "1"}], ",", 
             RowBox[{"st", "=", "1"}], ",", 
             RowBox[{
              RowBox[{"gh", "=", 
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"gh", "*", 
                  RowBox[{"PowerMod", "[", 
                   RowBox[{"g", ",", 
                    RowBox[{"-", "1"}], ",", "p"}], "]"}]}], ",", "p"}], 
                "]"}]}], ";", 
              RowBox[{"k", "++"}]}]}], "]"}]}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<k=\>\"", ",", "k"}], "]"}], ";", 
        RowBox[{"FactorInteger", "[", "gh", "]"}]}]}], "]"}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.8854724906941137`*^9, 3.885472491749057*^9}},
 CellLabel->
  "In[137]:=",ExpressionUUID->"e7f45735-09c6-430f-af64-6cb40718093e"],

