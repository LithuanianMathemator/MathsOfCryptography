           RowBox[{"NN", "=", 
            RowBox[{"MultiplicativeOrder", "[", 
             RowBox[{"g", ",", "p"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"n", "=", 
            RowBox[{
             RowBox[{"Round", "[", 
              RowBox[{"Sqrt", "[", "NN", "]"}], "]"}], "+", "1"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"gn", "=", 
            RowBox[{"PowerMod", "[", 
             RowBox[{"g", ",", 
              RowBox[{"-", "n"}], ",", "p"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"lst1", "=", 
            RowBox[{"{", "1", "}"}]}], ";", 
           RowBox[{"lst2", "=", 
            RowBox[{"{", "h", "}"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"gst", "=", "1"}], ";", 
           RowBox[{"hst", "=", "h"}], ";", 
           RowBox[{"ag", "=", "0"}], ";", 
           RowBox[{"ah", "=", "0"}], ";", 
           RowBox[{"st", "=", "0"}], ";", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"i", "=", "0"}], ",", 
             RowBox[{
              RowBox[{"st", "==", "0"}], "&&", 
              RowBox[{"i", "<", "n"}]}], ",", 
             RowBox[{"i", "++"}], ",", 
             RowBox[{
              RowBox[{"gst", "=", 
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"g", "*", "gst"}], ",", "p"}], "]"}]}], ";", 
              RowBox[{"lst1", "=", 
               RowBox[{"Append", "[", 
                RowBox[{"lst1", ",", "gst"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"MemberQ", "[", 
                 RowBox[{"lst2", ",", "gst"}], "]"}], ",", 
                RowBox[{
                 RowBox[{"st", "=", "1"}], ";", 
                 RowBox[{"ag", "=", 
                  RowBox[{"Position", "[", 
                   RowBox[{"lst2", ",", "gst"}], "]"}]}]}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"hst", "=", 
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"gn", "*", "hst"}], ",", "p"}], "]"}]}], ";", 
              RowBox[{"lst2", "=", 
               RowBox[{"Append", "[", 
                RowBox[{"lst2", ",", "hst"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"MemberQ", "[", 
                 RowBox[{"lst1", ",", "hst"}], "]"}], ",", 
                RowBox[{
                 RowBox[{"st", "=", "2"}], ";", 
                 RowBox[{"ah", "=", 
                  RowBox[{"Position", "[", 
                   RowBox[{"lst1", ",", "hst"}], "]"}]}]}]}], "]"}]}]}], 
            "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"st", "==", "1"}], ",", 
             RowBox[{"res", "=", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"ag", "[", 
                    RowBox[{"[", 
                    RowBox[{"1", ",", "1"}], "]"}], "]"}], "-", "1"}], ")"}], 
                  "*", "n"}], "+", "i"}], ",", "NN"}], "]"}]}], ",", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"st", "==", "2"}], ",", 
               RowBox[{"res", "=", 
                RowBox[{"Mod", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"ah", "[", 
                    RowBox[{"[", 
                    RowBox[{"1", ",", "1"}], "]"}], "]"}], "-", "1", "+", 
                   RowBox[{"i", "*", "n"}]}], ",", "NN"}], "]"}]}], ",", 
               RowBox[{"res", "=", "\"\<no solution\>\""}]}], "]"}]}], 
            "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", "res"}]}], "]"}]}], 
