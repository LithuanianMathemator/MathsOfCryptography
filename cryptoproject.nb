Cell[BoxData[
 RowBox[{
  RowBox[{"MillerRabin", "=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", "prob"}], "}"}], ",", " ", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "res", ",", "temp", ",", "i", ",", "T", " ", ",", "a", ",", "test", 
         ",", "dummy", ",", "k", ",", "q", ",", "check", ",", "h"}], "}"}], 
       ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"test", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
        RowBox[{"h", "=", 
         RowBox[{"Log", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"T", "=", "0"}], ";", "\[IndentingNewLine]", 
        RowBox[{"res", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
        RowBox[{"temp", " ", "=", " ", 
         RowBox[{"n", "-", "1"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"k", "=", "0"}], ";", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"IntegerQ", "[", 
           RowBox[{"temp", "/", "2"}], "]"}], ",", 
          RowBox[{
           RowBox[{"k", "++"}], ";", "\[IndentingNewLine]", " ", 
           RowBox[{"temp", "=", 
            RowBox[{"temp", "/", "2"}]}]}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"q", "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"n", "-", "1"}], ")"}], "/", 
          RowBox[{"(", 
           RowBox[{"2", "^", "k"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"EvenQ", "[", "n", "]"}], ",", 
          RowBox[{"res", " ", "=", " ", "1"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"test", "<", "prob"}], " ", "&&", " ", 
           RowBox[{"res", "==", "0"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"check", " ", "=", " ", "T"}], ";", "\[IndentingNewLine]", 
           
           RowBox[{"a", " ", "=", " ", 
            RowBox[{"RandomInteger", "[", 
             RowBox[{"{", 
              RowBox[{"2", ",", 
               RowBox[{"n", "-", "1"}]}], "}"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"GCD", "[", 
                 RowBox[{"n", ",", "a"}], "]"}], ">", "1"}], "&&", 
               RowBox[{
                RowBox[{"GCD", "[", 
                 RowBox[{"n", ",", "a"}], "]"}], " ", "<", " ", "n"}]}], 
              ")"}], ",", 
             RowBox[{"res", "=", "1"}]}], "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"a", " ", "=", " ", 
            RowBox[{"PowerMod", "[", 
             RowBox[{"a", ",", "q", ",", "n"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Mod", "[", 
               RowBox[{"a", ",", "n"}], "]"}], "==", "1"}], ",", " ", 
             RowBox[{"T", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{"For", "[", 
              RowBox[{
               RowBox[{"i", "=", "0"}], ",", 
               RowBox[{"i", "<", "k"}], ",", 
               RowBox[{"i", "++"}], ",", " ", 
               RowBox[{"If", "[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"PowerMod", "[", 
                   RowBox[{"a", ",", " ", 
                    RowBox[{
                    RowBox[{"2", "^", "i"}], "*", "q"}], ",", "n"}], "]"}], "==", 
                  RowBox[{"n", "-", "1"}]}], ",", " ", 
                 RowBox[{
                  RowBox[{"T", "++"}], ";", " ", 
                  RowBox[{"Break", "[", "]"}]}]}], "]"}]}], "]"}]}], "]"}], 
           ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"T", "-", "check"}], " ", "==", " ", "0"}], ",", 
             RowBox[{"res", " ", "=", " ", "1"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"test", "=", 
            RowBox[{"1", "-", 
             RowBox[{"h", "/", 
              RowBox[{"(", 
               RowBox[{"4", "^", "T"}], ")"}]}]}]}], ";"}]}], 
         "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"res", " ", "==", " ", "0"}], ",", " ", 
          RowBox[{
          "Print", "[", "\"\<There is a chance the number is prime!\>\"", 
           "]"}], ",", " ", 
          RowBox[{"Print", "[", "\"\<The number is composite!\>\"", "]"}]}], 
         "]"}]}]}], "\[IndentingNewLine]", "]"}]}], "]"}]}], ";"}]], "Input",
